{
    "name": "littlegreenman/draftable",
    "description": "Draftable - A library to quickly support users drafting content for your Laravel models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Elliot Ali",
            "email": "development@littlegreenman.software",
            "homepage": "https://littlegreenman.software"
        }
    ],
    "homepage": "https://github.com/littlegreenman/draftable",
    "keywords": ["Laravel", "Draftable"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LittleGreenMan\\Draftable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LittleGreenMan\\Draftable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LittleGreenMan\\Draftable\\DraftableServiceProvider"
            ],
            "aliases": {
                "Draftable": "LittleGreenMan\\Draftable\\Facades\\Draftable"
            }
        }
    }
}
